# Combine into data frame
df <- data.frame(Date, Economy_State_dummy, GDP, CPI, UNEMP_RATE, MLR)

# View the first few rows
head(df)

install.packages("VGAM")

library(VGAM)
gen_ord_logit <- vglm(Economy_State_dummy ~ UNEMP_RATE, family=genetic(vglm), data=df)
summary(gen_ord_logit)
